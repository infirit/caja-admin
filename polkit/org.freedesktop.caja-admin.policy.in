<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE policyconfig PUBLIC "-//freedesktop//DTD PolicyKit Policy Configuration 1.0//EN" "http://www.freedesktop.org/standards/PolicyKit/1/policyconfig.dtd">
<policyconfig>
    <vendor>Caja Admin</vendor>
    <vendor_url>https://github.com/infirit/caja-admin</vendor_url>
    <action id="org.freedesktop.pkexec.caja">
        <description>Run Caja as Root</description>
        <description xml:lang="ar">شغّل نوتلس كجذر</description>
        <description xml:lang="cs">Spustit aplikaci Caja s právy superuživatele</description>
        <description xml:lang="de">Caja als Aministrator ausführen</description>
        <description xml:lang="es">Ejecutar Caja como administrador</description>
        <description xml:lang="eu">Exekutatu Caja Root gisa</description>
        <description xml:lang="fr">Lancez Caja en tant qu'administrateur</description>
        <description xml:lang="it">Esegui Caja come Root</description>
        <description xml:lang="lt">Paleisti failų naršyklę kaip Administratorius</description>
        <description xml:lang="pl">Uruchom Caja jako Root</description>
        <description xml:lang="pt">Executar o Caja como Root</description>
        <description xml:lang="ru">Запустить Caja как Администратор</description>
        <description xml:lang="sk">Spustenie programu Caja ako správca</description>
        <message>Authentication is required to run the File Manager as Administrator</message>
        <message xml:lang="ar">المصادقة مطلوبة لتشغيل مدير الملفات كمدير</message>
        <message xml:lang="cs">Ke spuštění správce souborů s právy správce systému je vyžadováno ověření</message>
        <message xml:lang="de">Authentifizierung ist erforderlich um den Datei-Manager als Administrator auszuführen</message>
        <message xml:lang="es">Se necesita autentificación para ejecutar el Gestor de Ficheros como Administrador</message>
        <message xml:lang="eu">Autentifikazioa behar da fitxategi-arakatzailea administratzaile gisa exekutatzeko</message>
        <message xml:lang="fr">L'authentification est requise pour lancer le gestionnaire de fichiers en tant qu'administrateur</message>
        <message xml:lang="it">Autenticazione richiesta per lanciare il File Manager come Amministratore</message>
        <message xml:lang="lt">Failų naršyklei paleisti Administratoriaus teisėmis reikia įvesti slaptažodį</message>
        <message xml:lang="pl">Uwierzytelnienie jest wymagane aby uruchomić Menedżer Plików jako Administrator</message>
        <message xml:lang="pt">Autenticação é necessária para executar o Gestor de Ficheiros como Administrador</message>
        <message xml:lang="ru">Для запуска Файлового Менеджера от имени Администратора требуется аутентификация</message>
        <message xml:lang="sk">Na spustenie správcu súborov ako administrátor sa vyžaduje overenie totožnosti</message>
        <message xml:lang="zh_CN">以超级用户身份运行 Caja</message>
        <icon_name>system-file-manager</icon_name>
        <defaults>
            <allow_any>auth_admin</allow_any>
            <allow_inactive>auth_admin</allow_inactive>
            <allow_active>auth_admin</allow_active>
        </defaults>
        <annotate key="org.freedesktop.policykit.exec.path">@CAJA_PATH@</annotate>
        <annotate key="org.freedesktop.policykit.exec.allow_gui">true</annotate>
    </action>
    <action id="org.freedesktop.pkexec.pluma">
        <description>Open Pluma as Root</description>
        <description xml:lang="ar">افتح محرر النصوص Pluma كجذر</description>
        <description xml:lang="cs">Otevřít aplikaci Pluma s právy superuživatele</description>
        <description xml:lang="de">Öffne Pluma als Administrator</description>
        <description xml:lang="es">Abrir Pluma como Administrador</description>
        <description xml:lang="eu">Ireki Pluma Root gisa</description>
        <description xml:lang="fr">Ouvrir Pluma en tant qu'administrateur</description>
        <description xml:lang="it">Aprire Pluma come Root</description>
        <description xml:lang="lt">Atverti teksto failą kaip Administratorius</description>
        <description xml:lang="pl">Otwórz Pluma jako Root</description>
        <description xml:lang="pt">Abrir Pluma como Root</description>
        <description xml:lang="ru">Открыть Pluma как Администратор</description>
        <description xml:lang="sk">Otvorenie programu Pluma ako správca</description>
        <message>Authentication is required to run the Text Editor as Administrator</message>
        <message xml:lang="ar">المصادقة مطلوبة لتشغيل محرر النصوص كمدير</message>
        <message xml:lang="cs">Ke spuštění textového editoru s právy správce systému je vyžadováno ověření</message>
        <message xml:lang="de">Authentifizierung ist erforderlich um den Text-Editor als Administrator auszuführen</message>
        <message xml:lang="es">Se necesita autentificación para abrir el Editor de Texto como Administrador</message>
        <message xml:lang="eu">Autentifikazioa behar da testu-editorea administratzaile gisa exekutatzeko</message>
        <message xml:lang="fr">L'authentification est requise pour lancer l'éditeur de texte en tant qu'administrateur</message>
        <message xml:lang="it">Autenticazione richiesta per eseguire l'Editor di Testo come Amministratore</message>
        <message xml:lang="lt">Tekstų redagavimo programai paleisti Administratoriaus teisėmis reikia įvesti slaptažodį</message>
        <message xml:lang="pl">Uwierzytelnienie jest wymagane aby uruchomić Edytor tekstowy jako Administrator</message>
        <message xml:lang="pt">Autenticação é necessária para executar o Editor de Texto como Administrador</message>
        <message xml:lang="ru">Для запуска Текстового Редактора от имени Администратора требуется аутентификация</message>
        <message xml:lang="sk">Na spustenie textového editora ako administrátor sa vyžaduje overenie totožnosti</message>
        <message xml:lang="zh_CN">以超级用户身份打开 Pluma</message>
        <icon_name>accessories-text-editor</icon_name>
        <defaults>
            <allow_any>auth_admin</allow_any>
            <allow_inactive>auth_admin</allow_inactive>
            <allow_active>auth_admin</allow_active>
        </defaults>
        <annotate key="org.freedesktop.policykit.exec.path">@PLUMA_PATH@</annotate>
        <annotate key="org.freedesktop.policykit.exec.allow_gui">true</annotate>
    </action>
    <action id="org.freedesktop.pkexec.mateterminal">
        <description>Run as Root</description>
        <description xml:lang="ar">شغّل كجذر</description>
        <description xml:lang="cs">Spustit s právy superuživatele</description>
        <description xml:lang="de">Als Administrator ausführen</description>
        <description xml:lang="eu">Exekutatu Root gisa</description>
        <description xml:lang="fr">Exécuter en tant qu'administrateur</description>
        <description xml:lang="it">Eseguire come Root</description>
        <description xml:lang="lt">Vykdyti Administratoriaus teisėmis</description>
        <description xml:lang="pl">Uruchom jako Root</description>
        <description xml:lang="pt">Executar como Root</description>
        <description xml:lang="ru">Запустить как Администратор</description>
        <description xml:lang="sk">Spustenie ako správca</description>
        <message>Authentication is required to run as Administrator</message>
        <message xml:lang="ar">المصادقة مطلوبة للتشغيل كمدير</message>
        <message xml:lang="cs">Ke spuštění s právy správce systému je vyžadováno ověření</message>
        <message xml:lang="de">Authentifizierung ist erforderlich zum ausführen als Administrator</message>
        <message xml:lang="eu">Autentifikazioa behar da administratzaile gisa exekutatzeko</message>
        <message xml:lang="fr">L'authentification est requise pour exécuter en tant qu'administrateur</message>
        <message xml:lang="it">Autenticazione necessaria per eseguire come Amministratore</message>
        <message xml:lang="lt">Vykdomųjų failų paleidimui Administratoriaus teisėmis reikia įvesti slaptažodį</message>
        <message xml:lang="pl">Uwierzytelnienie jest wymagane aby uruchomić jako Administrator</message>
        <message xml:lang="pt">Autenticação é necessária para executar como Administrador</message>
        <message xml:lang="ru">Для запуска исполняемых файлов от имени Администратора требуется аутентификация</message>
        <message xml:lang="sk">Na spustenie ako administrátor sa vyžaduje overenie totožnosti</message>
        <message xml:lang="zh_CN">以超级用户身份运行</message>
        <icon_name>utilities-terminal</icon_name>
        <defaults>
            <allow_any>auth_admin</allow_any>
            <allow_inactive>auth_admin</allow_inactive>
            <allow_active>auth_admin</allow_active>
        </defaults>
        <annotate key="org.freedesktop.policykit.exec.path">@TERMINAL_PATH@</annotate>
        <annotate key="org.freedesktop.policykit.exec.allow_gui">true</annotate>
    </action>
</policyconfig>
